{
  "name": "nodcord",
  "version": "0.4.7",
  "description": "Eine TypeScript-basierte Express-API mit integriertem Discord-Bot und Prisma-MySQL-Backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --config nodemon.json",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:ci": "npm run lint && npm run typecheck && npm run test:coverage",
    "test:api": "jest --config jest.config.cjs --selectProjects api",
    "test:bot": "jest --config jest.config.cjs --selectProjects bot",
    "test:views": "jest --config jest.config.cjs --selectProjects views",
    "lint": "npm run lint:eslint && npm run lint:style && npm run lint:prettier",
    "lint:eslint": "eslint \"{src,prisma}/**/*.{ts,js}\" --max-warnings 0",
    "lint:style": "stylelint \"src/public/**/*.css\"",
    "lint:prettier": "prettier --check .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit && tsc --project tsconfig.jest.json --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vectode/NodCord.git"
  },
  "bugs": {
    "url": "https://github.com/vectode/NodCord/issues"
