{
  "name": "nodcord",
  "version": "0.4.7",
  "description": "A TypeScript-based Express API with integrated Discord bot and Prisma MySQL backend.",
  "main": "dist/server.js",
  "type": "module",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": "20.17.0",
    "npm": "11.6.2"
  },
  "files": [
    "src",
    "dist",
    "docs",
    "prisma",
    "README.md",
    "LICENSE",
    "SECURITY.md",
    "CONTRIBUTING.md"
  ],
  "scripts": {
    "dev": "nodemon --config nodemon.json",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:ci": "npm run lint && npm run typecheck && npm run test:coverage",
    "test:api": "jest --config jest.config.cjs --selectProjects api",
    "test:bot": "jest --config jest.config.cjs --selectProjects bot",
    "test:views": "jest --config jest.config.cjs --selectProjects views",
    "lint": "npm run lint:eslint && npm run lint:style && npm run lint:prettier",
    "lint:eslint": "eslint \"{src,prisma}/**/*.{ts,js}\" --max-warnings 0",
    "lint:style": "stylelint \"src/public/**/*.css\"",
    "lint:prettier": "prettier --check .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit && tsc --project tsconfig.jest.json --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "npx prisma db seed",
    "prestart": "npm run build",
    "postinstall": "npm run prisma:generate",
    "security:audit": "npm audit --production",
    "security:audit:fix": "npm audit fix --force"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VectoDE/NodCord.git"
  },
  "bugs": {
    "url": "https://github.com/VectoDE/NodCord/issues"
  },
  "homepage": "https://github.com/VectoDE/NodCord#readme",
  "keywords": [
    "nodejs",
    "express",
    "discord",
    "api",
    "bot",
    "typescript",
    "prisma",
    "mysql"
  ],
  "author": "Vecto. <timhauke@uplytech.de> (https://github.com/VectoDE)",
  "contributors": [
    "Contributor 1 <contributor1@example.com> (https://github.com/contributor1)",
    "Contributor 2 <contributor2@example.com> (https://github.com/contributor2)"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/VectoDE"
  },
