  "description": "Eine Node.js-API mit Express.js und einem integrierten Discord-Bot",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build-complete": "node build.js && npm run sentry:sourcemaps",
    "build-backend": "",
    "start:prod": "node build/start.js",
    "test": "jest",
    "prettify": "prettier --write .",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org uplytech-64 --project node-express ./build && sentry-cli sourcemaps upload --org uplytech-64 --project node-express ./build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
