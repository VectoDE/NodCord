<ul class="divide-y divide-gray-200 list-group">
  <% projects.forEach(project => { %>
    <li class="py-4 flex justify-between items-center list-group-item d-flex justify-content-between align-items-center">
      <span><%= project.name %></span>
      <span class="bg-green-500 text-white rounded-full px-3 py-1 badge badge-success rounded-pill">Updated: <%= project.updated_at %></span>
    </li>
  <% }) %>
</ul>
