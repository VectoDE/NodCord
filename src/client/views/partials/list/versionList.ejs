<ul class="divide-y divide-gray-200 list-group">
  <% versions.forEach(version => { %>
    <li class="py-4 flex justify-between items-center list-group-item d-flex justify-content-between align-items-center">
      <span><%= version.name %></span>
      <span class="bg-yellow-500 text-white rounded-full px-3 py-1 badge badge-warning rounded-pill">Released: <%= version.release_date %></span>
    </li>
  <% }) %>
</ul>
