<div id="editApiKeyModal" class="modal fade" tabindex="-1" aria-labelledby="editApiKeyModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content bg-gray-800 text-white">
      <div class="modal-header border-b border-gray-700">
        <h5 class="modal-title" id="editApiKeyModalLabel">Edit API Key</h5>
        <button type="button" class="btn-close text-gray-400" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <form id="editApiKeyForm">
          <div class="mb-3">
            <label for="editApiKeyName" class="form-label">API Key Name</label>
            <input type="text" class="form-control" id="editApiKeyName" value="<%= apiKeyName %>" required>
          </div>
          <button type="submit" class="btn btn-primary">Update API Key</button>
        </form>
      </div>
    </div>
  </div>
</div>
